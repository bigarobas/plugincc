<?xml version="1.0" encoding="UTF-8"?> 
<ExtensionList>
    <Extension Id="com.bigarobas.plugincc">
        <HostList>
           
            <!-- Comment Host tags according to the apps you want your panel to support -->
            
            <!-- Photoshop -->
            <Host Name="PHXS" Port="8088"/>
            <Host Name="PHSP" Port="8088" />
            <!-- InDesign -->
            <Host Name="IDSN" Port="8089" />
            <!-- Premiere -->
            <Host Name="PPRO" Port="8090" />
            <!-- AfterEffects -->
            <Host Name="AEFT" Port="8091" />
                        
        </HostList>
    </Extension>
</ExtensionList>
 
